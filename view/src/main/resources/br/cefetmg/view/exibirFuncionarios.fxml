<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.cefetmg.view.ExibirFuncionariosController">

    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>

    <TableView fx:id="tabela">
        <columns>
            <TableColumn fx:id="nomeCol" maxWidth="90" minWidth="90" text="Nome" />
            <TableColumn fx:id="telefoneCol" maxWidth="60" minWidth="60" text="Telefone" />
            <TableColumn fx:id="usernameCol" maxWidth="70" minWidth="70" text="Username" />
            <TableColumn fx:id="funcaoCol" maxWidth="70" minWidth="70" text="Função" />
        </columns>
    </TableView>
   <Button mnemonicParsing="false" onAction="#voltar" text="Voltar" />
</VBox>
